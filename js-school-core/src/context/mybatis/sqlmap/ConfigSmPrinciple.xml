<?xml version="1.0" encoding="UTF-8"?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- namespace：必须与对应的接口全类名一致 id:必须与对应接口的某个对应的方法名一致 --><mapper namespace="com.hikootech.mqcash.dao.ConfigSmPrincipleDAO">	<resultMap type="com.hikootech.mqcash.po.ConfigSmPrinciple" id="ConfigSmPrincipleResultMap">		<result property="principleId" column="principle_id"/>		<result property="principleTarget" column="principle_target"/>		<result property="principleTargetType" column="principle_target_type"/>		<result property="intervalDay" column="interval_day"/>		<result property="sendTime" column="send_time"/>		<result property="createTime" column="create_time"/>		<result property="updateTime" column="update_time"/>		<result property="operator" column="operator"/>	</resultMap>	<select id="querySmConfigByTargetType"  resultMap="ConfigSmPrincipleResultMap">		 select * from t_config_sms_principle where principle_target_type=#{value}	</select></mapper>