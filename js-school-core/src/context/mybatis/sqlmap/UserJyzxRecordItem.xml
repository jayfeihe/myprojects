<?xml version="1.0" encoding="UTF-8"?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- namespace：必须与对应的接口全类名一致 id:必须与对应接口的某个对应的方法名一致 --><mapper namespace="com.hikootech.mqcash.dao.UserJyzxRecordItemsDAO">	<resultMap type="com.hikootech.mqcash.po.UserJyzxRecordItem" id="UserJyzxRecordItemResultMap">		<result property="id" column="id"/>		<result property="trxNo" column="trx_no"/>		<result property="borrowType" column="borrow_type"/>		<result property="borrowState" column="borrow_state"/>		<result property="borrowAmount" column="borrow_amount"/>		<result property="contractDate" column="contract_date"/>		<result property="loanPeriod" column="loan_period"/>		<result property="repayState" column="repay_state"/>		<result property="arrearsAmount" column="arrears_amount"/>		<result property="companyCode" column="company_code"/>		<result property="createTime" column="create_time"/>	</resultMap>		<insert id="addUserJyzxRecordItem">		insert into t_user_jyzx_record_item			(id, trx_no, borrow_type, borrow_state, borrow_amount, contract_date, loan_period, repay_state, arrears_amount, company_code, create_time) 		values(#{id},#{trxNo},#{borrowType},#{borrowState},#{borrowAmount},#{contractDate}, #{loanPeriod},#{repayState},#{arrearsAmount},#{companyCode},#{createTime})	</insert>	<select id="queryUserJyzxRecordItemsByTrxNo" resultMap="UserJyzxRecordItemResultMap">		select * from t_user_jyzx_record_item 			where trx_no = #{value}  		order by  create_time desc	</select></mapper>