e" />		<result property="updateTime" column="update_time" />		<result property="batchNo" column="batch_no" />	</resultMap>	<insert id="addQhzxCredit" parameterType="com.hikootech.mqcash.po.UserQhzxCredit">		insert into		t_user_qhzx_credit(batch_no,id,id_no,id_type,reason_code,name,entity_auth_code,entity_auth_date,seq_no,mobile_no,card_no,email,weibo_no,weixin_no,qq_no,taobao_no,jd_no,amazon_no,yhd_no,source_id,credoo_score,bse_info_score,fin_require_score,pay_ability_score,perform_score,action_score,vir_asset_score,trend_score,data_build_time,er_code,er_msg,create_time,update_time,total_info_id,trans_no)		values(#{batchNo}, #{id}, #{idNo}, #{idType}, #{reasonCode}, #{name},		#{entityAuthCode}, #{entityAuthDate}, #{seqNo}, #{mobileNo},		#{cardNo}, #{email}, #{weiboNo}, #{weixinNo}, #{qqNo}, #{taobaoNo},		#{jdNo}, #{amazonNo}, #{yhdNo}, #{sourceId}, #{credooScore},		#{bseInfoScore}, #{finRequireScore}, #{payAbilityScore},		#{performScore}, #{actionScore}, #{virAssetScore}, #{trendScore},		#{dataBuildTime}, #{erCode}, #{erMsg}, #{createTime},		#{updateTime},#{totalInfoId},#{transNo})	</insert>	<select id="queryQhzxCreditByInfo" parameterType="com.hikootech.mqcash.po.UserQhzxCredit"		resultType="java.lang.Integer">		select count(*) from t_user_qhzx_credit		where id_no=#{idNo} and id_type=#{idType} and name=#{name} and		mobile_no=#{mobileNo}	</select>	<update id="updateQhzxCreditByInfo" parameterType="com.hikootech.mqcash.po.UserQhzxCredit">		UPDATE t_user_qhzx_credit		SET		batch_no=#{batchNo},		reason_code=#{reasonCode},		seq_no=#{seqNo},		source_id=#{sourceId},		credoo_score=#{credooScore},		bse_info_score=#{bseInfoScore},		fin_require_score=#{finRequireScore},		pay_ability_score=#{payAbilityScore},		perform_score=#{performScore},		action_score=#{actionScore},		vir_asset_score=#{virAssetScore},		trend_score=#{trendScore},		data_build_time=#{dataBuildTime},		er_code=#{erCode},		er_msg=#{erMsg},		update_time=#{updateTime}		where		id_no=#{idNo} and id_type=#{idType} and name=#{name} and		mobile_no=#{mobileNo}	</update>		<select id="queryQhzxCredit" resultMap="UserQhzxMsc8005ResultMap" >	select * from t_user_qhzx_credit		 where id_no= #{idNo} and  name=#{name} order by create_time desc	</select>	</mapper>