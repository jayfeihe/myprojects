<?xml version="1.0" encoding="UTF-8"?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- namespace：必须与对应的接口全类名一致 id:必须与对应接口的某个对应的方法名一致 --><mapper namespace="com.hikootech.mqcash.dao.UserRepaymentOrderDAO">	<resultMap type="com.hikootech.mqcash.po.UserRepaymentOrder" id="UserRepaymentOrderResultMap">		<result property="repaymentOrderId" column="repayment_order_id"/>		<result property="userId" column="user_id"/>		<result property="userPaymentOrderId" column="user_payment_order_id"/>		<result property="paymentType" column="payment_type"/>		<result property="paymentChannelId" column="payment_channel_id"/>		<result property="bankId" column="bank_id"/>		<result property="bankName" column="bank_name"/>		<result property="paymentAmount" column="payment_amount"/>		<result property="paymentCount" column="payment_count"/>		<result property="paymentTime" column="payment_time"/>		<result property="createTime" column="create_time"/>	</resultMap>	<insert id="addUserRepaymentOrderFromPayment" parameterType="com.hikootech.mqcash.po.UserRepaymentOrder">		INSERT INTO t_user_repayment_order			  (repayment_order_id, user_id, user_payment_order_id, payment_type, payment_channel_id, 		bank_id, bank_name, payment_amount, payment_count, payment_time, create_time,payment_status)	  	select #{repaymentOrderId},user_id, user_payment_order_id, payment_type, payment_channel_id, 		bank_id, bank_name, payment_amount, payment_count, payment_time, #{createTime},#{paymentStatus}	 from  t_user_payment_order where user_payment_order_id=#{userPaymentOrderId}	</insert></mapper>