<?xml version="1.0" encoding="UTF-8"?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- namespace：必须与对应的接口全类名一致 id:必须与对应接口的某个对应的方法名一致 --><mapper namespace="com.hikootech.mqcash.dao.CheckSchoolDAO">	<resultMap type="com.hikootech.mqcash.po.UserSchoolRoll" id="UserSchoolRollMap">		<result property="id" column="id"/>		<result property="requestNo" column="request_no"/>		<result property="documentNo" column="document_no"/>		<result property="name" column="name"/>		<result property="collegeLevel" column="college_level"/>		<result property="college" column="college"/>		<result property="startTime" column="start_time"/>		<result property="documentNoCheckResult" column="document_no_check_result"/>		<result property="nameCheckResult" column="name_check_result"/>		<result property="collegeLevelCheckResult" column="college_level_check_result"/>		<result property="collegeCheckResult" column="college_check_result"/>		<result property="startTimeCheckResult" column="start_time_check_result"/>		<result property="status" column="status"/>		<result property="createTime" column="create_time"/>		<result property="descp" column="descp"/>	</resultMap>	<insert id="insertCheckSchoolInfo" parameterType="com.hikootech.mqcash.po.UserSchoolRoll">		INSERT INTO  t_user_school_roll (			id,			request_no,			document_no,			name,			college_level,			college,			start_time,			document_no_check_result,			name_check_result,			college_level_check_result,			college_check_result,			start_time_check_result,			status,			create_time,			descp		)		VALUES			(				#{id},				#{requestNo},				#{documentNo},				#{name},				#{collegeLevel},				#{college},				#{startTime},				#{documentNoCheckResult},				#{nameCheckResult},				#{collegeLevelCheckResult},				#{collegeCheckResult},				#{startTimeCheckResult},				#{status},				#{createTime},				#{descp}			)	</insert>		<select id="queryUserSchoolRollInfo" resultMap="UserSchoolRollMap">		select t.* from t_user_school_roll t		where		 t.document_no = #{documentNo} order by t.create_time desc	</select></mapper>