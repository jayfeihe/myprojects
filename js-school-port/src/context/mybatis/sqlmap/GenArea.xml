<?xml version="1.0" encoding="UTF-8"?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- namespace：必须与对应的接口全类名一致 id:必须与对应接口的某个对应的方法名一致 --><mapper namespace="com.hikootech.mqcash.dao.GenAreaDAO">	<resultMap type="com.hikootech.mqcash.po.GenArea" id="GenAreaResultMap">		<result property="areaId" column="AREA_ID"/>		<result property="areaNo" column="AREA_NO"/>		<result property="areaName" column="AREA_NAME"/>		<result property="areaParentno" column="AREA_PARENTNO"/>		<result property="areaRank" column="AREA_RANK"/>		<result property="areaZipcode" column="AREA_ZIPCODE"/>	</resultMap>	<select id="getAreaList" resultMap="GenAreaResultMap">		select * from t_gen_area  s where s.AREA_PARENTNO=#{value}  order by s.AREA_ID;	</select></mapper>