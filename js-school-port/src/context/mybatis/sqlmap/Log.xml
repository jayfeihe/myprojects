<?xml version="1.0" encoding="UTF-8"?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- namespace：必须与对应的接口全类名一致 id:必须与对应接口的某个对应的方法名一致 --><mapper namespace="com.hikootech.mqcash.dao.LogDAO"><!-- 发送短信日志  --><insert id="addLogUserInfo" parameterType="com.hikootech.mqcash.po.LogUserInfo" >		insert into t_user_info_log(log_id,operation_type,operation_content,operation_result,descp,user_id,id_card,name,id_card_address,contact_phone,bind_mobile,pwd,pwd_level,sex,birthday,province_id,city_id,area_id,home_address,company_name,company_province_id,company_city_id,company_area_id,company_address,interesting,head_img_url,user_sm_type,status,create_time,update_time) 	values( #{logId},#{operationType},#{operationContent},#{operationResult},#{descp},#{userId},#{idCard},#{name},#{idCardAddress},#{contactPhone},#{bindMobile},#{pwd},#{pwdLevel},#{sex},#{birthday},#{provinceId},#{cityId},#{areaId},#{homeAddress},#{companyName},#{companyProvinceId},#{companyCityId},#{companyAreaId},#{companyAddress},#{interesting},#{headImgUrl},#{userSmType},#{status},#{createTime},#{updateTime}   )</insert><!-- 发送短信日志  --><insert id="insertSmLog" parameterType="com.hikootech.mqcash.po.SmLog">		INSERT INTO t_sms_log	(sms_order_id, mobile_number, data,send_type, book_time, send_time, send_status, template_id, create_time, update_time, operator)	VALUES (#{smOrderId}, #{mobileNumber}, #{data},#{sendType}, #{bookTime}, #{sendTime}, #{sendStatus}, #{templateId}, #{createTime}, #{updateTime}, #{operator})	</insert>		<!-- 登陆信息日志  -->	<insert id="addUserLoginRecord" parameterType="com.hikootech.mqcash.po.UserLoginRecord">		insert into t_user_login_log(			record_id,			login_account,			ip_address,			login_method,			login_result,			descp,			login_time,			create_time		)				values(			#{recordId},			#{loginAccount},			#{ipAddress},			#{loginMethod},			#{loginResult},			#{descp},			#{loginTime},			#{createTime}		)		</insert></mapper>