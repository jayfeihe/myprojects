<?xml version="1.0" encoding="UTF-8"?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- namespace：必须与对应的接口全类名一致 id:必须与对应接口的某个对应的方法名一致 --><mapper namespace="com.hikootech.mqcash.dao.UserProtocolDAO">	<resultMap type="com.hikootech.mqcash.po.UserProtocol" id="UserProtocolResultMap">		<result property="protocolId" column="protocol_id"/>		<result property="instalmentId" column="instalment_id"/>		<result property="providerId" column="provider_id"/>		<result property="userId" column="user_id"/>		<result property="userName" column="user_name"/>		<result property="idCard" column="id_card"/>		<result property="homeAddress" column="home_address"/>		<result property="userPhone" column="user_phone"/>		<result property="providerName" column="provider_name"/>		<result property="providerAddress" column="provider_address"/>		<result property="providerPhone" column="provider_phone"/>		<result property="instalmentCount" column="instalment_count"/>		<result property="overdueRate" column="overdue_rate"/>		<result property="perServiceRate" column="per_service_rate"/>		<result property="perServiceAmount" column="per_service_amount"/>		<result property="perServiceAmountZh" column="per_service_amount_zh"/>		<result property="partnerOrderAmount" column="partner_order_amount"/>		<result property="partnerOrderAmountZh" column="partner_order_amount_zh"/>		<result property="protocolAmount" column="protocol_amount"/>		<result property="protocolAmountZh" column="protocol_amount_zh"/>		<result property="productName" column="product_name"/>		<result property="protocolStatus" column="protocol_status"/>		<result property="protocolTemplateId" column="protocol_template_id"/>		<result property="enableTime" column="enable_time"/>		<result property="firstRepayTime" column="first_repay_time"/>		<result property="createTime" column="create_time"/>		<result property="updateTime" column="update_time"/>		<result property="operator" column="operator"/>	</resultMap>	<sql id="Column_List" >		protocol_id,instalment_id,provider_id,user_id,user_name,id_card,home_address,user_phone,provider_name,provider_address,provider_phone,instalment_count,overdue_rate,per_service_rate,per_service_amount,per_service_amount_zh,partner_order_amount,partner_order_amount_zh,protocol_amount,protocol_amount_zh,product_name,protocol_status,protocol_template_id,enable_time,first_repay_time,create_time,update_time,operator	</sql><insert id="addUserProtocol">	insert into t_user_protocol (protocol_id,instalment_id,provider_id,user_id,user_name,id_card,home_address,user_phone,provider_name,provider_address,provider_phone,instalment_count,overdue_rate,per_service_rate,per_service_amount,per_service_amount_zh,partner_order_amount,partner_order_amount_zh,protocol_amount,protocol_amount_zh,product_name,protocol_status,protocol_template_id,enable_time,first_repay_time,create_time,update_time,operator)	 	values	 	( #{protocolId}, #{instalmentId}, #{providerId}, #{userId}, #{userName}, #{idCard}, #{homeAddress}, #{userPhone}, #{providerName}, #{providerAddress}, #{providerPhone}, #{instalmentCount}, #{overdueRate}, #{perServiceRate}, #{perServiceAmount}, #{perServiceAmountZh}, #{partnerOrderAmount}, #{partnerOrderAmountZh}, #{protocolAmount}, #{protocolAmountZh}, #{productName}, #{protocolStatus}, #{protocolTemplateId}, #{enableTime}, #{firstRepayTime}, #{createTime}, #{updateTime}, #{operator})	</insert>				<!-- 通过用户id和分期id查询合同  -->	<select id="queryUserProtocolByInstalmentId" parameterType="java.util.Map" resultMap="UserProtocolResultMap" >		select <include refid="Column_List" /> 		 from t_user_protocol  where instalment_id = #{instalmentId} and user_id=#{userId}	</select></mapper>